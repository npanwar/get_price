<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_shopkeeper_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.dell_pc.get_price.ShopkeeperHome">
    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButtonAdditem"
        android:background="@drawable/aditem"
        android:layout_marginTop="93dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="57dp"
        android:layout_marginStart="57dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/delete"
        android:id="@+id/imageButtondelete"
        android:layout_alignTop="@+id/imageButtonAdditem"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="39dp"
        android:layout_marginEnd="39dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButtonUpdate"
        android:background="@drawable/update"
        android:layout_marginTop="80dp"
        android:layout_below="@+id/imageButtonAdditem"
        android:layout_alignLeft="@+id/imageButtonAdditem"
        android:layout_alignStart="@+id/imageButtonAdditem" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/search"
        android:id="@+id/imageButtonStalk"
        android:layout_alignTop="@+id/imageButtonUpdate"
        android:layout_alignLeft="@+id/imageButtondelete"
        android:layout_alignStart="@+id/imageButtondelete" />
</RelativeLayout>

